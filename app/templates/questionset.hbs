{{#if isNext}}
  <h4><button disabled={{isDisabled}} {{action "next"}}>Next</button></h4>
{{/if}}

{{#if isEnd}}
  <h4><button disabled={{isDisabled}} {{action "end"}}>End</button></h4>
{{/if}}
<!-- Vinay code start -->
<div>
  <div class="container col-xs-10">

    <form class="well form-horizontal" action=" " method="post" id="contact_form">
      <fieldset>

        <!-- Form Name -->
        <legend>
          <center>
            <h2><b>Please Start your survey </b></h2></center>
        </legend><br>

        <div class="form-group">
          <label class="col-md-4 control-label"></label>
          <div class="col-md-8 inputGroupContainer">
            <div class="input-group col-md-4">
              {{#if isStart}}
                <!-- <h4><button {{action "start"}}>Start</button></h4 -->
                <button class="btn btn-warning btn-lg btn-block" Style="width: 200px;" {{action "start"}}>Start Survey</button>
              {{/if}}
            </div>
          </div>
        </div>

      </fieldset>
    </form>
  </div>
</div>

