{{#if isClicked}}
  <h3>{{number}}{{' '}}{{question}}</h3>
  <h4>{{#radio-button
  value= optionA
  groupValue=ans
  changed="selectedAns"}}
  {{' '}}{{ansA}}
{{/radio-button}}</h4>

  <h4>{{#radio-button
  value= optionB
  groupValue=ans
  changed="selectedAns"}}
  {{' '}}{{ansB}}
{{/radio-button}}</h4>

  <h4>{{#radio-button
  value= optionC
  groupValue=ans
  changed="selectedAns"}}
  {{' '}}{{ansC}}
{{/radio-button}}</h4>

{{/if}}

<h2>{{message}}</h2>

